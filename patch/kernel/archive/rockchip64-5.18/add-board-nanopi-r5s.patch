+	status = "okay";
+};
+
+// 1G ethernet(WAN)
+&gmac0 {
+	status = "okay";
+
+	assigned-clocks = <&cru SCLK_GMAC0_RX_TX>, <&cru SCLK_GMAC0>;
+	assigned-clock-parents = <&cru SCLK_GMAC0_RGMII_SPEED>, <&cru CLK_MAC0_2TOP>;
+	assigned-clock-rates = <0>, <125000000>;
+	clock_in_out = "output";
+	phy-handle = <&rgmii_phy0>;
+	// phy-mode = "rgmii-id";
+	pinctrl-names = "default";
+	pinctrl-0 = <&gmac0_miim
+		     &gmac0_tx_bus2
+		     &gmac0_rx_bus2
+		     &gmac0_rgmii_clk
+		     &gmac0_rgmii_bus>;
+
+	// following setting taken from friendship kernel
+	snps,reset-gpio = <&gpio0 RK_PC5 GPIO_ACTIVE_LOW>;
+	snps,reset-active-low;
+	/* Reset time is 15ms, 50ms for rtl8211f */
+	snps,reset-delays-us = <0 15000 50000>;
+
+	phy-mode = "rgmii";
+	tx_delay = <0x3c>;
+	rx_delay = <0x2f>;
+};
+
+// gmac0/mdio0
+&mdio0 {
+	rgmii_phy0: phy@1 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <0x1>;
+		interrupt-parent = <&gpio0>;
+		interrupts = <RK_PC4 IRQ_TYPE_LEVEL_LOW>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&gmac_int>;
+	};	
+};
+
+&gic {
+	its: interrupt-controller@fd440000 {
+		compatible = "arm,gic-v3-its";
+		msi-controller;
+		#msi-cells = <1>;
+		reg = <0x0 0xfd440000 0x0 0x20000>;
+	};
+};
+
+// 2.5G Ethernet(LAN1)
+// used by pcie2x1
+&combphy2 {
+	status = "okay";
+};
+
+&pcie2x1 {
+	num-viewport = <4>;
+	reset-gpios = <&gpio0 RK_PB6 GPIO_ACTIVE_HIGH>;
+	status = "okay";
+};
+
+// pcie phy for pcie3x1 && pcie3x2
+&pcie30phy {
+	rockchip,pipe-grf = <&pipegrf>;
+	status = "okay";
+};
+
+// 2.5G Ethernet (LAN2)
+&pcie3x1 {
+	lane-map = <1 0>; // replace rockchip,bifurcation
+	reset-gpios = <&gpio0 RK_PA0 GPIO_ACTIVE_HIGH>;
+	status = "okay";
+};
+
+// NVME
+&pcie3x2 {
+	// lane-map = <0 1>; // not required, &pcie3x1 enabled bifurcation
+	// num-lanes = <1>; // not required, although default value is 2 in rk3568.dtsi
+	reset-gpios = <&gpio2 RK_PD6 GPIO_ACTIVE_HIGH>;
+	vpcie3v3-supply = <&vcc3v3_pcie>;
+	status = "okay";
+};
+
+// used by usb_host0_xhci
+&combphy0 {
+	status = "okay";
+};
+
+// used by usb_host1_xhci
+&combphy1 {
+	status = "okay";
+};
+
+&usb_host0_ehci {
